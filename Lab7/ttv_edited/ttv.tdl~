; might not be true.

aux41-aux-lex := subj-raise-aux-no-pred &
  [ SYNSEM.LOCAL [ CONT.HOOK.INDEX.E.MOOD irr,
                   CAT.VAL [ SUBJ.FIRST.LOCAL.CONT.HOOK.INDEX.PNG [ PER 3rd,
                                                                    NUM sg ],
                             COMPS.FIRST.LOCAL.CAT.HEAD.FORM finite ] ] ].

; To keep the semantically empty ones from spinning on
; generation, require complement to be [AUX -].  The
; FORM feature might be enough in the starter grammars,
; but I don't want to rely on this.  Then again, [ AUX - ]
; might not be true.

aux42-aux-lex := subj-raise-aux-no-pred &
  [ SYNSEM.LOCAL [ CONT.HOOK.INDEX.E.MOOD irr,
                   CAT.VAL [ SUBJ.FIRST.LOCAL.CONT.HOOK.INDEX.PNG [ PER person,
                                                                    NUM non-sg ],
                             COMPS.FIRST.LOCAL.CAT.HEAD.FORM finite ] ] ].

; To keep the semantically empty ones from spinning on
; generation, require complement to be [AUX -].  The
; FORM feature might be enough in the starter grammars,
; but I don't want to rely on this.  Then again, [ AUX - ]
; might not be true.

aux43-aux-lex := subj-raise-aux-no-pred &
  [ SYNSEM.LOCAL [ CONT.HOOK.INDEX.E.MOOD realis,
                   CAT.VAL [ SUBJ.FIRST.LOCAL.CONT.HOOK.INDEX.PNG [ PER 1st,
                                                                    NUM pl ],
                             COMPS.FIRST.LOCAL.CAT.HEAD.FORM finite ] ] ].

;;;;;;;;;;;;;;;;;;;
;;; Subordinators
;;;;;;;;;;;;;;;;;;;

adposition-subord-lex-item := single-rel-lex-item & norm-ltop-lex-item & basic-icons-lex-item &
  [ SYNSEM.LOCAL.CAT [ MC -,
                       HEAD adp &
                            [ MOD < [ LOCAL scopal-mod &
                                            [ CAT [ HEAD verb,
                                                    VAL.COMPS < > ] ] ] > ],
                       VAL [ SUBJ < >,
                             SPR < >,
                             SPEC < >,
                             COMPS < [ OPT -,
                                       LOCAL.CAT [ MC -,
                                                   VAL.COMPS < > ] ] > ] ] ].

subord-with-verbal-comp-lex := adposition-subord-lex-item &
  [ SYNSEM [ LOCAL [ CAT [ HEAD.MOD < [ LOCAL [ CAT.HEAD verb,
                                                CONT.HOOK [ LTOP #mod,
                                                            INDEX #index ] ] ] >,
                           VAL.COMPS < [ LOCAL [ CAT.HEAD verb,
                                                 CONT.HOOK.LTOP #comps ] ] > ],
                     CONT [ HCONS.LIST < qeq &
                                         [ HARG #h1,
                                           LARG #mod ],
                                         qeq &
                                         [ HARG #h2,
                                           LARG #comps ] >,
                            HOOK.INDEX #index ] ],
             LKEYS.KEYREL [ ARG1 #h1,
                            ARG2 #h2 ] ] ].

irr-clause-final-prehead-s-attach-subord-lex-item := subord-with-verbal-comp-lex &
  [ SYNSEM.LOCAL [ CONT.HOOK.INDEX.E.MOOD #feat,
                   CAT [ VAL.COMPS < [ LOCAL [ CAT.VAL.SUBJ < >,
                                               CONT.HOOK.INDEX.E.MOOD #feat & irr ] ] >,
                         POSTHEAD -,
                         HEAD [ MOD < [ LOCAL.CAT.VAL.SUBJ < > ] >,
                                INIT - ] ] ] ].

realis-clause-init-prehead-s-attach-subord-lex-item := subord-with-verbal-comp-lex &
  [ SYNSEM.LOCAL [ CONT.HOOK.INDEX.E.MOOD #feat,
                   CAT [ VAL.COMPS < [ LOCAL [ CAT.VAL.SUBJ < >,
                                               CONT.HOOK.INDEX.E.MOOD #feat & realis ] ] >,
                         POSTHEAD -,
                         HEAD [ MOD < [ LOCAL.CAT.VAL.SUBJ < > ] >,
                                INIT + ] ] ] ].

clause :+ [ SYNSEM.NON-LOCAL.QUE.LIST < > ].

;;; Complementizers

;;; Others

norm-adposition-lex := norm-sem-lex-item & no-hcons-lex-item & basic-intersective-mod-lex & non-local-none-lex-item &
  [ ARG-ST < #comp >,
    SYNSEM [ LKEYS.KEYREL arg12-ev-relation &
                          [ ARG1 #arg1,
			    ARG2 #arg2 ],
             L-QUE #lque,
             LOCAL [ CONT [ HOOK [ XARG #arg1 ] ],
                     CAT [ WH.BOOL -,
                           VAL [ SPR < >,
                                 SPEC < >,
                                 SUBJ < [ LOCAL [ CAT cat-sat &
                         	      	  	    	    [ VAL [ SPR < >,
                                 			    COMPS < > ],
                          				     HEAD noun ],
					          CONT.HOOK.INDEX #arg1 ] ] >,
                                 COMPS < #comp &
                                         [ L-QUE #lque,
                                           LOCAL [ CAT [ HEAD noun,
                                                         VAL.SPR < > ],
                                                   CONT.HOOK.INDEX #arg2 ] ] > ],
                           HEAD adp &
                                [ MOD < [ LOCAL.CAT [ VAL.SPR cons,
                                                      WH.BOOL -,
                                                      HEAD.AUX - ] ] > ] ] ] ] ].

normadp1-norm-adposition-lex := norm-adposition-lex &
  [ SYNSEM.LOCAL.CAT.HEAD.INIT + ].

;;; Determiners
;;; SPEC is non-empty, and already specified by basic-determiner-lex.

determiner-lex := basic-determiner-lex & basic-non-wh-word-lex & norm-zero-arg & non-mod-lex-item &
  [ SYNSEM.LOCAL.CAT.VAL [ SPR < >,
                           COMPS < >,
                           SUBJ < > ] ].

;;; Adverbs

;;;;;;;;;;;;;;;;;;;
;;; Lexical Rules
;;;;;;;;;;;;;;;;;;;

; From Lab 7

n-bar-predicate-rule := unary-phrase & nocoord &
  [ SYNSEM [ LOCAL.CAT [ HEAD verb,
                   VAL [ COMPS < >,
			 SUBJ < [ LOCAL [ CONT.HOOK.INDEX #arg1,
					  CAT [ HEAD noun,
						VAL.SPR < > ] ] ] > ] ],
             NON-LOCAL #nl ],
    C-CONT [ HOOK [ LTOP #ltop,
            INDEX #index,
            XARG #arg1 ],
         RELS.LIST < arg12-ev-relation &
           [ PRED "_be_v_id_rel",
             LBL #ltop,
             ARG0 #index,
             ARG1 #arg1,
             ARG2 #arg2 ],
           quant-relation &
           [ PRED "exist_q_rel",
             ARG0 #arg2,
             RSTR #harg ] >,
         HCONS.LIST < qeq & [ HARG #harg, LARG #larg ] > ],
    ARGS < [ SYNSEM [ LOCAL [ CAT [ HEAD noun,
                     VAL.SPR cons ],
                  CONT.HOOK [ INDEX #arg2,
                                      LTOP #larg ]],
                  NON-LOCAL #nl ] ] > ].


; Adjective Incorporation

adj_incorporation-lex-rule := add-only-rule &
  [ C-CONT [ RELS.LIST < arg1-ev-relation &
                         [ LBL #ltop,
                           ARG1 #index ] >,
             HOOK #hook ],
    DTR.SYNSEM.LOCAL [ CAT.HEAD noun,
                       CONT.HOOK #hook &
                                 [ LTOP #ltop,
                                   INDEX #index ] ] ].

;;; Intermediate rule types

verb-pc11-rule-dtr := word-or-lexrule.
verb-pc12-rule-dtr := word-or-lexrule.

;;; Lexical rule types

noun-pc3-lex-rule-super := lex-rule &
  [ INFLECTED [ NOUN-PC3-FLAG +,
                VERB-PC10-FLAG #verb-pc10,
                VERB-PC11-OR-VERB-PC12-FLAG #verb-pc11-or-verb-pc12,
                VERB-PC12-FLAG #verb-pc12,
                VERB-PC8-FLAG #verb-pc8,
                VERB-PC3-FLAG #verb-pc3,
                VERB-PC5-FLAG #verb-pc5,
                VERB-PC9-FLAG #verb-pc9,
                VERB-PC11-FLAG #verb-pc11 ],
    DTR noun-lex &
        [ INFLECTED [ VERB-PC10-FLAG #verb-pc10,
                      VERB-PC11-OR-VERB-PC12-FLAG #verb-pc11-or-verb-pc12,
                      VERB-PC12-FLAG #verb-pc12,
                      VERB-PC8-FLAG #verb-pc8,
                      VERB-PC3-FLAG #verb-pc3,
                      VERB-PC5-FLAG #verb-pc5,
                      VERB-PC9-FLAG #verb-pc9,
                      VERB-PC11-FLAG #verb-pc11 ] ] ].

noun-pc3_lrt1-lex-rule := infl-lex-rule & noun-pc3-lex-rule-super &
  [ SYNSEM.LOCAL.CAT [ HEAD #head,
                       VAL [ SPR < >,
                             SPEC #spec,
                             SUBJ #subj,
                             COMPS #comps ] ],
    C-CONT [ HOOK #hook,
             RELS.LIST < noun-relation &
                         [ PRED "pron_rel",
                           LBL #lbl2,
                           ARG0 #possessor &
                                [ PNG [ NUM du,
                                        PER 1st ],
                                  COG-ST activ-or-more,
                                  SPECI + ] ],
                         quant-relation &
                         [ PRED "exist_q_rel",
                           ARG0 #possessum,
                           RSTR #harg ],
                         arg12-ev-relation &
                         [ PRED "poss_rel",
                           LBL #lbl,
                           ARG1 #possessum,
                           ARG2 #possessor ],
                         quant-relation &
                         [ PRED "exist_q_rel",
                           ARG0 #possessor,
                           RSTR #harg2 ] >,
             HCONS.LIST < qeq &
                          [ HARG #harg,
                            LARG #lbl ],
                          qeq &
                          [ HARG #harg2,
                            LARG #lbl2 ] > ],
    DTR.SYNSEM.LOCAL [ CONT.HOOK #hook &
                                 [ INDEX #possessum &
                                         [ COG-ST activ-or-more ],
                                   LTOP #lbl ],
                       CAT [ HEAD #head &
                                  [ PRON - ],
                             VAL [ SPEC #spec,
                                   SUBJ #subj,
                                   COMPS #comps ] ] ] ].

